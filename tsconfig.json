{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "declaration": true,
    "strictNullChecks": false,
    "paths": {
      "native": ["native"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}